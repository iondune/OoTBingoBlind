<!doctype html>

<html lang="en">
<head>
  <meta charset="utf-8">

  <title>OoT Tool</title>
  <meta name="description" content="oot betaquest blind bingo lockout">
  <meta name="author" content="Ian Dunn">

  <link href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">
  <link rel="stylesheet" href="/css/styles.css?v=1.0">

  <!--[if lt IE 9]>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/html5shiv/3.7.3/html5shiv.js"></script>
  <![endif]-->
</head>

<body>
<div class="container">
  <h1><%= title %> Results</h1>
  <p><%= roomId %></p>
  <p><a href="/rooms/<%= roomId %>">Back To Room</a></p>

  <div id="squares">
    <ul>
    <% for (var field in squares) { %>
      <% if (squares.hasOwnProperty(field)) { %>
        <li><%= field %> - <%= squares[field] %></li>
      <% } %>
    <% } %>
    </ul>

  </div>

  <script src="https://code.jquery.com/jquery-3.2.1.min.js"></script>
  <script src="/js/oot_beautify.js"></script>
  <script src="/js/custom_list.js"></script>

  </div>
</body>
</html>
